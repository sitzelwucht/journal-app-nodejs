
<div id="content-flex">
    <div id="header"><h1>tags</h1>
        <a href="/users/logout" id="logout"><img src="./logout.svg" id="logout-icon" height=15" title="Log out"></a>
    </div>
        
    <div id="center-box">
        <%- include('./partials/navbar.ejs') %>
        <div id="content-box">
            <% for (let i = results.length-1; i > 0; i--) { %>
                <div class="entry-text">
                <div class="date"><%= results[i].date.toLocaleString() %></div>
                <div class="post-header"><%= results[i].title %></div>
                <div class="post"><%= results[i].description %></div>
            <% if (results[i].tag) { %> 
                <div class="tag"><%= results[i].tag %></div>
                <% } %>
            </div> 
            <% } %>
        </div>
    </div>
</div>