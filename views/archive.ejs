
<div id="content-flex">
    <div id="header"><h1>archive</h1>
        <a href="/users/logout" id="logout"><img src="./logout.svg" id="logout-icon" height=15" title="Log out"></a>
    </div>
        
    <div id="center-box">
        <%- include('./partials/navbar.ejs') %>
        <div id="content-box">
            <% if (entries.length == 1) { %>
                <div class="entry-text">
                    <div class="date"><%= entries[0].date.toLocaleString() %></div>
                    <div class="post-header"><%= entries[0].title %></div>
                    <div class="post"><%= entries[0].description %></div>
    
                <% if (entries[i].tags.length > 0) { %> 
                    <% for (let j = 0; j < entries[0].tags.length; j++) { %>
                        <span class="tag"><a href=""><%= entries[i].tags[j] %></a></span>
                        <% } %>
                    <% } %>

             <% }  %>

            <% if (entries.length > 1) { %>
            <% for (let i = entries.length-1; i > 0; i--) { %>
                <div class="entry-text">
                <div class="date"><%= entries[i].date.toLocaleString() %></div>
                <div class="post-header"><%= entries[i].title %></div>
                <div class="post"><%= entries[i].description %></div>

            <% if (entries[i].tags.length > 0) { %> 
                <% for (let j = 0; j < entries[i].tags.length; j++) { %>
                    <span class="tag"><a href=""><%= entries[i].tags[j] %></a></span>
                    <% } %>
                <% } %>
            </div> 
            <% } } %>
        </div>
    </div>
</div>

  
